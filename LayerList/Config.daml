          <!-- host controls within groups -->
          <comboBox refID="LayerList_GetLayerFile" />
          <button refID="LayerList_Button1" size="large" />
          <comboBox refID="LayerList_ComboBox_LayerList" />
          <button refID="LayerList_Button2" size="large" />
        </group>
      </groups>
